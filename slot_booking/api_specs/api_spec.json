{
	"swagger": "2.0",
	"host": "localhost:8000",
	"basePath": "/api/slot_booking/",
	"info": {
		"version": "1.0.0",
		"title": "Washing Mechine Slot Booking",
		"description": "mini_project(assignment_1)"
	},
	"schemes": [
		"https",
		"http"
	],
	"consumes": [
		"application/json"
	],
	"produces": [
		"application/json"
	],
	"securityDefinitions": {
		"oauth": {
			"tokenUrl": "http://auth.ibtspl.com/oauth2/",
			"flow": "password",
			"scopes": {
				"read": "read users",
				"write": "create users",
				"update": "update users",
				"delete": "delete users",
				"superuser": "super user permission"
			},
			"type": "oauth2"
		}
	},
	"definitions": {
		"login_user": {
			"type": "object",
			"properties": {
				"username": {
					"type": "string"
				},
				"password": {
					"type": "string"
				}
			},
			"required": [
				"username",
				"password"
			]
		},
		"mechine_id": {
			"type": "object",
			"properties": {
				"washing_mechine_id": {
					"type": "integer",
					"format": "int32"
				}
			},
			"required": [
				"washing_mechine_id"
			]
		},
		"list_of_machine_details": {
			"type": "object",
			"properties": {
				"washing_machine_id": {
					"type": "integer",
					"format": "int32"
				},
				"is_active": {
					"type": "boolean"
				}
			},
			"required": [
				"washing_machine_id",
				"is_active"
			]
		},
		"total_washing_machines_details": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/list_of_machine_details"
			}
		}
	},
	"parameters": {
		"UserdetailsParameter": {
			"name": "user_details",
			"in": "body",
			"schema": {
				"$ref": "#/definitions/login_user"
			}
		},
		"WashingMechinedetailsParameter": {
			"name": "user_details",
			"in": "body",
			"schema": {
				"$ref": "#/definitions/mechine_id"
			}
		}
	},
	"responses": {
		"GenerateAccesstokenResponse": {
			"description": "",
			"schema": {
				"type": "object",
				"properties": {
					"access_token": {
						"type": "string"
					}
				},
				"required": [
					"access_token"
				]
			}
		},
		"GetAllWashingMachineDetailsResponse": {
			"description": "",
			"schema": {
						"$ref": "#/definitions/total_washing_machines_details"
				}
		}
	},
	"paths": {
		"/user/login/": {
			"post": {
				"security": [],
				"operationId": "get_access_Token",
				"description": "Delete given prefilled code",
				"parameters": [{
					"$ref": "#/parameters/UserdetailsParameter"
				}],
				"responses": {
					"200": {
						"$ref": "#/responses/GenerateAccesstokenResponse"
					},
					"404": {
						"description": "Invalid username or password"
					}
				}
			}
		},
		"/add/new_washing_mechine/": {
			"post": {
				"operationId": "Create_New_Washing_Mechine",
				"description": "Create New Washing Mechine",
				"security": [{
					"oauth": [
						"write"
					]
				}],
				"parameters": [{
					"$ref": "#/parameters/WashingMechinedetailsParameter"
				}],
				"responses": {
					"200": {
						"description": "Successfully Added"
					},
					"404": {
						"description": "Invalid Mechine_id"
					}
				}
			}
		},
		"/get/all_washing_mechine_detalis": {
			"get": {
				"security": [{
					"oauth": [
						"read"
					]
				}],
				"operationId": "Get_All_Washing_Mechine_Details",
				"description": "Get all washing mechine details",
				"parameters": [],
				"responses": {
					"200": {
						"$ref": "#/responses/GetAllWashingMachineDetailsResponse"
					}
				}
			}
		}
	}
}